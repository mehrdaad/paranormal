{
  "name": "paranormal",
  "version": "0.1.3",
  "description": "Phenomenal Code Examples",
  "main": "index.js",
  "bin": "bin.js",
  "repository": "https://github.com/thejameskyle/paranormal",
  "author": "James Kyle <me@thejameskyle.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist",
    "flow": "flow",
    "test": "ava",
    "format": "prettier --write src/**/*.js",
    "build:legacy": "BABEL_ENV=legacy babel src -Dd dist/legacy",
    "build:modern": "BABEL_ENV=modern babel src -Dd dist/modern",
    "build": "yarn run clean && yarn build:legacy && yarn build:modern",
    "dev": "yarn run clean && yarn build:modern --watch",
    "prepublish": "yarn build",
    "precommit": "lint-staged"
  },
  "keywords": [
    "example",
    "examples",
    "component",
    "components",
    "utils",
    "ui",
    "react",
    "vue",
    "angular",
    "ember",
    "storybook",
    "storybooks",
    "website"
  ],
  "files": [
    "bin.js",
    "index.js",
    "dist"
  ],
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "chalk": "^2.3.0",
    "chokidar": "^2.0.2",
    "find-up": "^2.1.0",
    "globby": "^8.0.1",
    "meow": "^4.0.0",
    "micromatch": "^3.1.10",
    "parcel-bundler": "^1.7.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "resolve-from": "^4.0.0",
    "rimraf": "^2.6.2",
    "shuri": "^1.0.1",
    "signal-exit": "^3.0.2",
    "spawndamnit": "^1.0.0",
    "strip-indent": "^2.0.0",
    "styled-components": "^3.2.3",
    "tempy": "^0.2.1",
    "typeable-promisify": "^2.0.1"
  },
  "devDependencies": {
    "ava": "^0.24.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "fixturez": "^1.0.1",
    "flow-bin": "^0.66.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "prettier": "^1.10.2"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  }
}
